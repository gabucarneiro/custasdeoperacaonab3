        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="calcular"
            android:textSize="20sp"
            android:textAllCaps="true"
            android:layout_marginTop="10dp"
            android:onClick="calcular">
        </Button>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1">
                <TextView
                    style="@style/Conteudo"
                    android:text="Valor de compra com custas:"
                    android:gravity="center">
                </TextView>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_gravity="center">
                    <TextView
                        style="@style/Conteudo"
                        android:layout_marginStart="15dp"
                        android:text="R$">
                    </TextView>
                    <TextView
                        android:id="@+id/valCompraDoPapel2"
                        style="@style/Conteudo"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:hint="0,00"
                        android:textColor="@color/darkred">
                    </TextView>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1">
                <TextView
                    style="@style/Conteudo"
                    android:text="Valor de venda menos custas:"
                    android:gravity="center">
                </TextView>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_gravity="center">
                    <TextView
                        style="@style/Conteudo"
                        android:layout_marginStart="15dp"
                        android:text="R$">
                    </TextView>
                    <TextView
                        android:id="@+id/valVendaDoPapel2"
                        style="@style/Conteudo"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:hint="0,00"
                        android:textColor="@color/darkgreen">
                    </TextView>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:id="@+id/LLSaldoVendaLiquido">
            <TextView
                style="@style/Conteudo"
                android:text="Saldo pós venda do papel:"
                android:layout_gravity="center">
            </TextView>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_gravity="center">
                <TextView
                    style="@style/Conteudo"
                    android:layout_marginStart="15dp"
                    android:text="R$">
                </TextView>
                <TextView
                    android:id="@+id/resultVendaLiquido"
                    style="@style/Conteudo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:hint="0,00">
                </TextView>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_gravity="center">
            <TextView
                style="@style/Conteudo"
                android:text="Valor mínimo para venda sem perdas: R$ "
                android:textSize="15sp">
            </TextView>
            <TextView
                android:id="@+id/valMinVendaSemPerdas"
                style="@style/Conteudo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:hint="0,00"
                android:textSize="15sp">
            </TextView>
        </LinearLayout>


        <!--  ***** CUSTOS OPERACIONAIS *****  -->

        <Button
